<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pxc.bandapanda"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="15" />
  <uses-permission android:name="android.permission.INTERNET" /> 
    <application
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/TemaPropi.Negre" >
     
        <activity
            android:name="BandaPanda"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Search" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
                <category android:name="android.intent.category.APP_MUSIC"/>
            </intent-filter>
        </activity>
        <activity android:name="MusicPlayer" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MUSICPLAYER"/>
                <category android:name="android.intent.category.APP_MUSIC"/>
            </intent-filter>
        </activity>
        <activity android:name="PlayActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.PLAYACTIVITY"/>
            </intent-filter>
            <intent-filter></intent-filter>
        </activity>
        <activity android:name="MenuPlaylist" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MUSIC_PLAYER"/>
                <category android:name="android.intent.category.APP_MUSIC"/>
            </intent-filter>
        </activity>
        
    </application>

</manifest>